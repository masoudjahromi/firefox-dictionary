{
  "manifest_version":2,
  "name":"Flashcard",
  "version":"1.0",
  "description":"Flashcard for learning",

  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":[
        "data/jquery.min.js",
        "data/flashcard.js"
      ],
      "css":[
        "data/styles.css"
      ]
    }
  ],
  "permissions": [
    "<all_urls>",
    "webRequest"
  ]
}
